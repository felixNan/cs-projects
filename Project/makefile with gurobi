/*this is the makefile,under it will be what we started with*/
COMP_FLAG = -ansi -O3 -Wall -Wextra -Werror -pedantic-errors
OBJS = main.o MainAux.o parser.o solver.o game.o fileFunc.o linkedListFunc.o stackFunc.o
GUROBI_COMP = -I/usr/local/lib/gurobi563/include /*find if this is the correct filepath*/
GUROBI_LIB = -L/usr/local/lib/gurobi563/lib -lgurobi56 /*same as above*/

sudoku-console: $(OBJS)
	gcc -o $@ $(OBJS) $(GUROBI_LIB)

main.o: main.c parser.h MainAux.h gameStructs.h SPBufferset.h
	gcc $(COMP_FLAG) $(GUROBI_COMP) -c $*.c

MainAux.o: MainAux.c MainAux.h gameStructs.h
	gcc $(COMP_FLAG) $(GUROBI_COMP) -c $*.c

parser.o: parser.c parser.h game.h gameStructs.h MainAux.h fileFunc.h solver.h
	gcc $(COMP_FLAG) $(GUROBI_COMP) -c $*.c

game.o: game.c game.h parser.h MainAux.h solver.h gameStructs.h linkedListFunc.h
	gcc $(COMP_FLAG) $(GUROBI_COMP) -c $*.c

solver.o: solver.c solver.h game.h gameStructs.h linkedListFunc.h stackFunc.h MainAux.h
	gcc $(COMP_FLAG) $(GUROBI_COMP) -c $*.c

fileFunc.o: fileFunc.c fileFunc.h gameStructs.h linkedListFunc.h solver.h game.h
	gcc $(COMP_FLAG) $(GUROBI_COMP) -c $*.c


linkedListFunc.o: linkedListFunc.c linkedListFunc.h gameStructs.h
	gcc $(COMP_FLAG) $(GUROBI_COMP) -c $*.c


stackFunc.o: stackFunc.c stackFunc.h gameStructs.h
	gcc $(COMP_FLAG) $(GUROBI_COMP) -c $*.c

clean:
	rm -f $(OBJS)
/*changed: 1.comp_flag 2.exe name*/
/*merge this*/
CC = gcc
OBJS = main.o
 
EXEC = myprog
COMP_FLAGS = -ansi -O3 -Wall -Wextra -Werror -pedantic-errors
GUROBI_COMP = -I/usr/local/lib/gurobi563/include
GUROBI_LIB = -L/usr/local/lib/gurobi563/lib -lgurobi56

$(EXEC): $(OBJS)
	$(CC) $(OBJS) $(GUROBI_LIB) -o $@ -lm
main.o: main.c
	$(CC) $(COMP_FLAGS) $(GUROBI_COMP) -c $*.c
clean:
	rm -f *.o $(EXEC) /*$(EXEC) also removes the exe and not only the .o files which are not needed to run,only the exe*/
	/**.o means all files that end with .o in the directory*/
/*into this*/
COMP_FLAG = -ansi -Wall -Wextra -Werror -pedantic-errors
OBJS = main.o MainAux.o parser.o solver.o game.o fileFunc.o linkedListFunc.o stackFunc.o

sudoku: $(OBJS)
	gcc -o $@ $(OBJS)

main.o: main.c parser.h MainAux.h gameStructs.h SPBufferset.h
	gcc $(COMP_FLAG) -c $*.c

MainAux.o: MainAux.c MainAux.h gameStructs.h
	gcc $(COMP_FLAG) -c $*.c

parser.o: parser.c parser.h game.h gameStructs.h MainAux.h fileFunc.h solver.h
	gcc $(COMP_FLAG) -c $*.c

game.o: game.c game.h parser.h MainAux.h solver.h gameStructs.h linkedListFunc.h
	gcc $(COMP_FLAG) -c $*.c

solver.o: solver.c solver.h game.h gameStructs.h linkedListFunc.h stackFunc.h MainAux.h
	gcc $(COMP_FLAG) -c $*.c

fileFunc.o: fileFunc.c fileFunc.h gameStructs.h linkedListFunc.h solver.h game.h
	gcc $(COMP_FLAG) -c $*.c


linkedListFunc.o: linkedListFunc.c linkedListFunc.h gameStructs.h
	gcc $(COMP_FLAG) -c $*.c


stackFunc.o: stackFunc.c stackFunc.h gameStructs.h
	gcc $(COMP_FLAG) -c $*.c

clean:
	rm -f $(OBJS)
